<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		
		
	<bean id="campus" class="main.model.facility.Campus">
		<constructor-arg value = "Loyola"></constructor-arg>
		</bean>
		
	<bean id="defaultRoom" class="main.model.facility.Room">
		<constructor-arg type="java.lang.String">
			<value>711</value>
		</constructor-arg>
		<constructor-arg type="int">
			<value>40</value>
		</constructor-arg>
		</bean>
		
	<bean id="defaultRoom2" class="main.model.facility.Room">
		<constructor-arg type="java.lang.String">
			<value>712</value>
		</constructor-arg>
		<constructor-arg type="int">
			<value>50</value>
		</constructor-arg>
		</bean>
		
	
	<bean id="defaultBuilding" class="main.model.facility.Building">
		<constructor-arg type="java.lang.String">
			<value>Corboy Law Center</value>
		</constructor-arg>
	</bean>
	
	<bean id="defaultMaintTicket" class="main.model.maintenance.MaintTicket">
		
	</bean>
	
	<bean id="MaterialA" class="main.model.maintenance.Material" name="brick">
		<property name="name" value="brick" />
		<property name="cost" value="2.00" />
	</bean>
	
	<bean id="MaterialB" class="main.model.maintenance.Material" name="bulb">
		<property name="name" value="bulb" />
		<property name="cost" value="5.00" />
	</bean>
	
	<bean id="WorkerA" class="main.model.maintenance.Worker" name="Bill">
		<property name="name" value="Bill" />
		<property name="wage" value="10" />
	</bean>
	
	<bean id="WorkerB" class="main.model.maintenance.Worker" name="Bob">
		<property name="name" value="Bob" />
		<property name="wage" value="20" />
	</bean>
		
		
	
		
</beans>